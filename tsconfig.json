{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"rootDir": "./src",
		"moduleResolution": "node",
		"outDir": "./out",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"resolveJsonModule": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"skipLibCheck": true
	}
}
